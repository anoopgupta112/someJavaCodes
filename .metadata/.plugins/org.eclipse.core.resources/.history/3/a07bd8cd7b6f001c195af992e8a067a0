package HomeWork;

public class recoursive {
	
	public static void main(String[] args) {
		
		String Str = "abcbc";
		
		int[] arr = {1,2,3,2,4,1,5,8};
	
		
		for(int i = 0; i<Str.length();i++) {
			for(int j =0;j>i;j--) {
			if(Str.charAt(i)==Str.charAt(j)) {
				System.out.println(Str);
			}	
			else {
				System.out.println("false");

			}
			}
			
			
			
		}
	}

}
