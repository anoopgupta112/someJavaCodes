package marrigeDate;

public class childclass1{
	public static void main(String[] args) {
		int a =3, b=3;
		int sum =  addnum(a,b);
		int fact = factorial(a);
		System.out.println(sum);
		System.out.println(fact);
	
	}
	public static int addnum(int a,int b) {
		return a+b;
	}
	public static int factorial(int a) {
		
		return a*2;
	}

}
