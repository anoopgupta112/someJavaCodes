package githubPretice;

public class sorting {
	
	static void Bubblesort() {
		int[] arr = {5,2,9,3,2,9,-1};
		int n = arr.length;
		for(int i = 0;i<n;i++) {
			for(int j = 0;j<(n-1)-i;j++) {
				if(arr[j]>arr[j+1]) {
					int temp = arr[j];
					arr[j]= arr[j+1];
					arr[j+1] = temp;
					
				}
			}
		}
		for(int elements: arr) {
			System.out.println(elements);
		}
	}
public static void main(String[] args) {
	Bubblesort();
}


}
